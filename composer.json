{
  "name": "makallio85/cake-yaml",
  "description": "Yaml routes for CakePHP3",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": ">=3.0.0",
    "symfony/yaml": "~3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "CakeYaml\\": "src/CakeYaml/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CakeYaml\\Test\\": "test/CakeYaml/Test/"
    }
  },
  "minimum-stability": "dev"
}
